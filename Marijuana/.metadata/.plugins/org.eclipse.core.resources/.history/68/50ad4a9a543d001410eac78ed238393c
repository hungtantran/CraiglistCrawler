package marijuanaCrawler;

import java.util.Map;

public class CraiglistCrawler {
	public static void main(String[] args) {
		CraiglistLocationLinkCrawl crawler = new CraiglistLocationLinkCrawl();
		
		// Fail to get the craiglist location links
		if (!crawler.getLink()) {
			System.out.println("Fail to get craiglist location links");
			return;
		}
		
		Map<Set<String>, String> linkToStateMap = crawler.getLinkToStateMap();
		
		if (linkToStateMap == null)
			return;
		
		
	}
}
