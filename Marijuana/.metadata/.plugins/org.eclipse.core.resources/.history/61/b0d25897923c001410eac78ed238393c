package marijuanaCrawler;

public class CraiglistCrawler {
	public static void main(String[] args) {
		CraiglistLocationLinkCrawl crawler = new CraiglistLocationLinkCrawl();
		
		// Fail to get the craiglis state links
		if (!crawler.getLink()) {
			System.out.println("Fail to get craiglist location links");
			return;
		}
		
		
	}
}
