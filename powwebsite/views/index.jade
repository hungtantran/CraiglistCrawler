doctype html
html. // THAT DOT
  <head>
    <link rel="stylesheet" type="text/css" href="/stylesheets/index.css">
    <script type="text/javascript"
      src="/javascripts/common.js" ></script>
    <script type="text/javascript">
      // Create marker
      var markers = [];
      latitude = 39.6948;
      longtitude = -104.7881;

      for (var i = 0; i < 10; ++i) {
        latitude += 0.2;
        longtitude -= 0.2;

        var marker = [];
        marker['latitude'] = latitude;
        marker['longtitude'] = longtitude;

        markers[i] = marker;
      }

      google.maps.event.addDomListener(window, 'load', function() {
          initialize(markers);
        }
      );
    </script>
  </head>
  <body>
    <div id="header">
    Header
    </div>

    <div id="content">
      <div id="left_pane">
        Average Price by State
        <div id="map-canvas">
        </div>

        <div id="price_bin">
          <div id="price_bin_dist_by_state">
            Price Distribution by State
          </div>

          <div id="price_per_unit">
            Price (per gram) (bin)
          </div>
        </div>
      </div>

      <div id="right_pane">
      Latest Prices
      </div>
    </div>

    <div id="footer">
    Footer
    </div>
  </body>
